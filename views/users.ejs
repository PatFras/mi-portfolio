<!DOCTYPE html>
<html lang="en">
<%- include('partials/head', { title : 'Users' }) %>

    <body>
        <%- include('partials/navbar') %>

            <%- include('partials/header', { title : 'Users' , page : 'home' , clase : 'bgimg-1' }) %>

                <div class="mvc-content mvc-container mvc-padding-64" id="users">
                    <h3 class="mvc-center">Users</h3>
                    <hr>
                    <ul>
                        <% users.forEach(user => { %>
                            <li><a href="/users/profile/<%= user.id %>"><%= user.name %></a></li>
                        <% }); %> 
                    </ul>
                </div>

    </body>

</html>